// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = "file:./dev.db"
}

// Universal Unique ID
model Feedback {
  id String @id @default(uuid()) //Pk

  type       String
  comment    String
  screenshot String?

  @@map("feedbacks")
}
